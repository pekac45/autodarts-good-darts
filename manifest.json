{
  "name": "Autodarts Helper",
  "description": "Automatically clicks the 'Good darts! ðŸŽ¯' button on autodarts.io matches",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [],
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["*://play.autodarts.io/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 